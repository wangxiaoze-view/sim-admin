{
  "name": "rsbuild-test",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "rsbuild build",
    "dev": "rsbuild dev --open",
    "preview": "rsbuild preview",
    "preinstall": "node ./scripts/preinstall.js",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V",
    "lint:fix": "eslint {src,mock,library}/**/*.{vue,js,ts} --fix",
    "lint:format": "prettier  {src,mock,library}/**/*.{html,vue,css,sass,scss,js,ts,md} --write ",
    "lint:style": "stylelint {src,mock,library}/**/*.{vue,css,sass,scss} --cache --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.10.0",
    "@rsbuild/core": "^1.0.1",
    "@rsbuild/plugin-vue": "^1.0.1",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.5.0"
  },
  "lint-staged": {
    "*.{js,ts,vue,json}": [
      "npm run lint:fix",
      "npm run lint:format",
      "npm run lint:style",
      "git add"
    ]
  },
  "web-types": "./web-types.json",
  "__npminstall_done": false
}
