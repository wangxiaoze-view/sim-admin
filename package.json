{
  "name": "sim-admin",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev --open --env-mode development",
    "build": "rsbuild build --env-mode production && node build/index.js",
    "build:profile": "RSDOCTOR=true RSPACK_PROFILE=ALL  rsbuild build --env-mode production",
    "build:analyze": "BUNDLE_ANALYZE=true rsbuild build --env-mode production",
    "preview": "rsbuild preview",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V",
    "lint:oxlint": "oxlint {src,mock,library}/**/*.{vue,js,ts} --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint --no-error-on-unmatched-pattern {src,mock,library}/**/*.{vue,js,ts} --fix",
    "lint:format": "prettier --no-error-on-unmatched-pattern  {src,mock,library}/**/*.{html,vue,css,sass,scss,js,ts,md} --write ",
    "lint:style": "stylelint {src,mock,library}/**/*.{vue,css,sass,scss} --cache --fix",
    "lint": "run-s lint:*",
    "commitlog": "conventional-changelog -p angular -i COMMIT.md -s -r 0 -n ./commitlog-option.mjs",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "release-it"
  },
  "dependencies": {
    "@log-reporting/core": "^1.0.8",
    "@logicflow/core": "^2.0.7",
    "@logicflow/extension": "^2.0.11",
    "@tinymce/tinymce-vue": "^6.1.0",
    "@tresjs/cientos": "^4.0.3",
    "@tresjs/core": "^4.3.1",
    "@tresjs/leches": "^0.14.0",
    "@vue-office/docx": "^1.6.2",
    "@vue-office/excel": "^1.7.11",
    "@vue-office/pdf": "^2.0.9",
    "async-validator": "^4.2.5",
    "axios": "^1.7.7",
    "colorthief": "^2.6.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "element-plus": "^2.8.7",
    "functional-helpers": "git@github.com:wangxiaoze-view/functional-helpers-lib.git",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.6",
    "print-js": "^1.6.0",
    "qrcode": "^1.5.4",
    "remixicon": "^4.5.0",
    "rrweb": "2.0.0-alpha.4",
    "rrweb-player": "1.0.0-alpha.4",
    "three": "^0.170.0",
    "vue": "^3.5.12",
    "vue-draggable-plus": "^0.6.0",
    "vue-i18n": "^10.0.4",
    "vue-picture-cropper": "^0.7.0",
    "vue-qrcode": "^2.2.2",
    "vue-router": "^4.4.5",
    "vue3-excel-editor": "^1.0.59",
    "vxe-pc-ui": "^4.2.51",
    "vxe-table": "^4.8.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.10.0",
    "@log-reporting/types": "^1.0.3",
    "@release-it/conventional-changelog": "^9.0.2",
    "@rsbuild/core": "^1.0.1",
    "@rsbuild/plugin-css-minimizer": "^1.0.2",
    "@rsbuild/plugin-image-compress": "^1.0.2",
    "@rsbuild/plugin-sass": "^1.0.2",
    "@rsbuild/plugin-vue": "^1.0.1",
    "@rsdoctor/cli": "^0.4.6",
    "@rsdoctor/rspack-plugin": "^0.4.6",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.7.5",
    "@types/nprogress": "^0.2.3",
    "@types/qrcode": "^1.5.5",
    "@types/three": "^0.170.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.0.1",
    "commitizen": "^4.3.1",
    "compare-func": "^2.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "conventional-changelog": "^6.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "cz-message-helper": "^1.3.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-oxlint": "^0.11.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.29.0",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "mockjs": "^1.1.0",
    "npm-run-all2": "^7.0.1",
    "oxlint": "^0.11.0",
    "postcss-html": "^1.7.0",
    "postcss-preset-env": "^10.0.7",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "release-it": "^17.10.0",
    "rspack-plugin-mock": "^0.5.0",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "5.5.2",
    "typescript-eslint": "^8.5.0",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vue-eslint-parser": "^9.4.3"
  },
  "lint-staged": {
    "*.{js,ts,vue,json}": [
      "pnpm run lint:oxlint",
      "pnpm run lint:eslint",
      "pnpm run lint:format",
      "git add"
    ]
  },
  "web-types": "./web-types.json",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-message-helper"
    },
    "cz-message-helper": {
      "config": "./.cz-message.cjs"
    }
  }
}
